      "source_content": "# 📦 theme-classic",
      "source_content_hash": "0587716d55250342e0158ce113a1b168752505c2a525bb7f00c38d26aecdacb6",
      "node_type": "heading",
      "translatable": true,
      "translations": {
        "zh-hant": "# 📦 經典主題"
      }
    },
    {
      "segment_id": "50387553",
      "source_content": "import APITable from '@site/src/components/APITable';",
      "source_content_hash": "617594357be14491b59c35a0417f7d0d89d9eda936d09d760f6b994379006923",
      "node_type": "mdxjsEsm",
      "translatable": false,
